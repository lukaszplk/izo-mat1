# Minimal .htaccess for LiteSpeed testing
RewriteEngine On

# Basic PHP extension removal
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]

# Default index
DirectoryIndex index.php index.html

# Allow all access (for testing)
<Directory "/home/asdqwe/public_html">
    AllowOverride All
    Require all granted
</Directory>
